extends layout

block content
  label UUID:
  select.uuid
  label Timestamp:
  input(type="text", placeholder="From").from
  input(type="text", placeholder="To").to
  input(type="text", placeholder="Limit").limit
  button(type="button").go Go
  p GPS
  .gps#map
  p Acceleromete
    span.accCount
  .acc.plot
  p Magnetometer
    span.magCount
  .mag.plot
  p Gyroscope
    span.gyroCount
  .gyro.plot
  p Tags
  table(border="1").tags
    thead
      tr
        th Time
        th Text
  p Wifi
  table(border="1").wifi
    thead
      tr
        th Time
        th BSSID
        th SSID
        th cap
        th connected
        th Frequency
        th Signal Level
  p GSM
  table(border="1").gsm
    thead
      tr
        th Time
        th Operator
        th lac
        th cid
        th rssi
  p Bluetooth
  table(border="1").bluetooth
    thead
      tr
        th Time
        th Address
        th Class
        th Name
        th rssi
