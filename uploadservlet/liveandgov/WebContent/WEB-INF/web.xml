<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>liveandgov</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/liveandgov</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <description></description>
    <display-name>Upload</display-name>
    <servlet-name>Upload</servlet-name>
    <servlet-class>de.uni_koblenz.west.mobile_sensing.Upload</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>JsonApi</display-name>
    <servlet-name>JsonApi</servlet-name>
    <servlet-class>de.uni_koblenz.west.mobile_sensing.JsonApi</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ShowData</display-name>
    <servlet-name>ShowData</servlet-name>
    <servlet-class>de.uni_koblenz.west.mobile_sensing.ShowData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/Upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JsonApi</servlet-name>
    <url-pattern>/JsonApi</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>ShowData</servlet-name>
    <url-pattern>/ShowData</url-pattern>
  </servlet-mapping>
</web-app>
